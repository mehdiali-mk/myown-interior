    "scss:watch": "sass --no-source-map --watch scss:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "sass": "^1.78.0"
  }
}
